<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="lo9618.dinesocial.StartPage">

    <TextView
        android:id= "@+id/intro"
        android:layout_width="200dp"
        android:layout_height="200dp"
        android:layout_marginTop="13dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginLeft="68dp"
        android:layout_marginStart="68dp" />
    <Button
        android:id="@+id/get_help"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Get Help"
        tools:layout_editor_absoluteX="124dp"
        tools:layout_editor_absoluteY="67dp"
        android:layout_centerVertical="true"
        android:layout_alignLeft="@+id/intro"
        android:layout_alignStart="@+id/intro" />

    <Button
        android:id="@+id/journal"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Journal"
        tools:layout_editor_absoluteX="124dp"
        tools:layout_editor_absoluteY="67dp"
        android:layout_below="@+id/get_help"
        android:layout_alignLeft="@+id/get_help"
        android:layout_alignStart="@+id/get_help"
        android:layout_marginTop="20dp" />

    <Button
        android:id="@+id/settings"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Settings"
        tools:layout_editor_absoluteX="124dp"
        tools:layout_editor_absoluteY="67dp"
        android:layout_below="@+id/journal"
        android:layout_alignLeft="@+id/journal"
        android:layout_alignStart="@+id/journal"
        android:layout_marginTop="24dp" />


</RelativeLayout>
